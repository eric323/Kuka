<apex:component >

  <script type="text/ng-template" id="base.html">
    <nav class="navbar navbar-default header-content">
      <div class="container">
        <a class="navbar-brand navbar-right" href="#">
          <img alt="Brand" src="{!URLFOR($Resource.APTSMD_Kuka, 'images/logo.png')}"/>
        </a>
        <div class="navbar-header">
          <p class="navbar-text" ng-if="model.isValidUser == 'true'">
            {{user}}
          </p>
          <p class="navbar-text" ng-if="model.isValidUser != 'true'">
            <a data-toggle="modal" data-target="#login-modal">{!$Label.APTSMD_Login}</a>
          </p>
          <p class="navbar-text">
            <img src="{{model.userLogoUrl}}" id="user-logo"/>
          </p>
          <p class="navbar-text cart-hover">
            <i class="cart-icon"></i>
            <span ng-show="cart.cartWrapper.cartLineItems.length>0" class="badge bg-danger">{{cart.cartWrapper.cartLineItems.length}}</span>
          </p>
          <p class="navbar-text">
            <a data-toggle="modal" class="text-muted" data-target="#language-modal">
              <i class="icon-Icon_069"></i>
            </a>
          </p>
        </div>
      </div>
    </nav>
    <content ui-view="banner"></content>

    <content ui-view="content"></content>

    <content ui-view="sub-footer"></content>

    <footer id="footer">
        <div class="kuka-container container">
          <div id="footer-image"></div>
          <ul class="linkholder socials">
            <li>
              <a href="{!$Label.APTSDMP_YoutubeURL}" target="_blank"><img src="{!URLFOR($Resource.APTSMD_FooterYoutubeGray)}" /></a>
            </li>
            <li><a href="{!$Label.APTSDMP_LinkedIn}" target="_blank"><img src="{!URLFOR($Resource.APTSMD_FooterLinkedinGray)}"/></a></li>
            <li><a href="{!$Label.APTSDMP_Facebook}" target="_blank"><img src="{!URLFOR($Resource.APTSMD_FooterFacebookGray)}" /></a></li>
            <li><a href="{!$Label.APTSDMP_Twitter}" target="_blank"><img src="{!URLFOR($Resource.APTSMD_FooterTwitterGray)}"/></a></li>
          </ul>
          <ul class="linkholder">
            <li>&copy; <a href="{!$Label.APTSDMP_KUKALink}">{!$Label.APTSDMP_KUKAAG}</a></li>
            <li><a href="{!$Label.APTSDMP_KUKAImprintURL}">{!$Label.APTSDMP_Imprint}</a></li>
            <li><a href="#!/legal/">{!$Label.CommunitySelfReg_HeaderTandD}</a></li>
          </ul>
      </div>
    </footer>

    <div class="modal fade" id="language-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="myModalLabel">{!$Label.APTSDMP_Language}</h4>
          </div>
          <div class="modal-body">
              <div class="list-group">
                <a href="javascript:void(0);" data-dismiss="modal" ng-click="selectLanguage('en_US')" class="list-group-item">{!$Label.APTSDMP_English}</a>
                <a href="javascript:void(0);" data-dismiss="modal" ng-click="selectLanguage('de')" class="list-group-item">{!$Label.APTSDMP_German}</a>
              </div>
          </div>
        </div>
      </div>
    </div>

    <div class="modal fade" id="login-modal" tabindex="-1" role="dialog">
      <div class="modal-dialog" role="document">
        <form name="loginForm" novalidate ng-submit="doLogin()">
          <div class="modal-content">
            <div class="modal-header text-center">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <h4 class="modal-title" id="myModalLabel">{!$Label.APTSMD_Login}</h4>
              <p class="m-b-n-xxs">
                {!$Label.APTSDMP_NotRegistered} <a href="{!$Label.KUKA_Ecom_Reg_URL}" style="font-size:16px;color:#ff6000;">{!$Label.APTSDMP_CreateAccount}</a>
              </p>
            </div>
            <div class="modal-body p-xl">

                <div class="{{loginForm.$invalid ? 'form-group has-error' : 'form-group'}}">
                  <label for="exampleInputEmail1">{!$Label.kuka_username}</label>
                  <input type="email" class="form-control" id="exampleInputEmail1" ng-model="loginModel.username" placeholder="{!$Label.kuka_username}" required>
                </div>
                <div class="{{loginForm.$invalid ? 'form-group has-error' : 'form-group'}}">
                  <label for="exampleInputPassword1">{!$Label.APTSMD_Password}</label>
                  <input type="password" class="form-control" id="exampleInputPassword1" ng-model="loginModel.password" placeholder="{!$Label.APTSMD_Password}" required ng-min="4">
                </div>
                <p class="helpBlock text-danger text-center m-b-n-md" ng-if="loginModel.error">{{loginModel.error}}</p>
            </div>
            <div class="modal-footer">
              <p class="pull-left m-l-sm m-b-n-sm m-t-xs">
                <a href="{!$Label.KUKA_Ecom_ForgotPass_URL}">{!$Label.APTSDMP_ForgotPassword}</a>
              </p>
              <button type="button" class="btn btn-default" data-dismiss="modal">{!$Label.APTSMD_Close}</button>
              <button type="submit" class="btn btn-primary" ng-disabled="loginForm.$invalid">{!$Label.APTSMD_Login}</button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </script>

  <script type="text/ng-template" id="subscribe.html">
    <section class="subscribe">
        <div class="kuka-container">
            <div class="title-holder">
                <h2 style="font-weight: normal; font-size: 33px;">{!$Label.APTSDMP_SubscribeIm}
                    <span class="text-primary">{!$Label.APTSDMP_SubscribeInterested}</span>
                </h2>
            </div>
            <div class="content-holder">
                <p>{!$Label.APTSDMP_SubscribeTagline}</p>
            </div>
            <form action="" method="POST" class="subscribe-form">
                <input type="email" name="email" class="input-field" placeholder="{!$Label.APTSDMP_EnterEmail}"/>
                <input type="submit" class="submit-button" value="{!$Label.APTSDMP_Submit}"/>
            </form>
        </div>
    </section>
  </script>

  <script type="text/ng-template" id="banner.html">
    <div id="promo-carousel" class="carousel slide m-b-xl" data-ride="carousel" data-interval="5000">
      <ol class="carousel-indicators">
        <li data-target="#promo-carousel" data-slide-to="{{$index}}" class="{{$index == 0 ? 'active' : ''}}" ng-repeat="promo in model.promoImages track by $index"></li>
      </ol>
      <div class="carousel-inner" role="listbox">
        <div class="{{$index == 0 ? 'item active' : 'item'}}" ng-repeat="promo in model.promoImages track by $index">
          <img ng-src="{{'servlet/servlet.FileDownload?file=' + promo}}"/>
        </div>
      </div>
    </div>
  </script>

