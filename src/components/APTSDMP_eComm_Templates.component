            <button type="button" class="btn btn-default" data-dismiss="modal">{!$Label.APTSMD_Close}</button>
            <button type="button" class="btn btn-primary" ng-disabled="productForm.$invalid" ng-click="addToCart()">{!$Label.APTSMD_AddToCart}</button>
          </div>
        </div>
      </div>
