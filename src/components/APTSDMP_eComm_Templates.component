                  <li data-target="#product-image-carousel" data-slide-to="2" ng-if="product.productDetails.APTSDMP_Slider_Pic_3__c"></li>
                  <li data-target="#product-image-carousel" data-slide-to="3" ng-if="product.productDetails.APTSDMP_Slider_Pic_4__c"></li>
