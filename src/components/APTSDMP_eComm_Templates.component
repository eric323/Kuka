                  <label for="{{attribute.name}}">{{attribute.label}}</label>
                  <select id="{{attribute.name}}" ng-if="attribute.fieldType.toUpperCase() == 'PICKLIST'"
                      ng-model="attribute.selectedValue"
                      ng-options="item.split(',')[0] for item in attribute.availableValues" class="form-control" required></select>

                  <input id="{{attribute.name}}" ng-if="attribute.fieldType.toUpperCase() == 'TEXT'" name="{{attribute.label}}" type="text" ng-model="attribute.selectedValue" class="form-control" required/>
                  <input id="{{attribute.name}}" ng-if="attribute.fieldType.toUpperCase() == 'DOUBLE'" name="{{attribute.label}}" type="number" min="0" ng-model="attribute.selectedValue" class="form-control" required/>
                  <input id="{{attribute.name}}" ng-if="attribute.fieldType.toUpperCase() == 'BOOLEAN'" name="{{attribute.label}}" type="checkbox" ng-model="attribute.selectedValue" class="form-control kuka-checkbox" required/>
                  <input id="{{attribute.name}}" ng-if="attribute.fieldType.toUpperCase() == 'DATE'" name="{{attribute.label}}" type="date" ng-min="today" ng-model="attribute.selectedValue" class="form-control" required/>
                  <small class="help-block" ng-if="productForm[attribute.label].$error.min">{!$Label.APTSDMP_ConfigError}&nbsp;{{attribute.label}}</small>
                </div>
              </div>
            </form>
